extends ../layouts/signedin-layout

block content
	.container
		.row-fluid
			.span12
				.span3
					ul.nav.nav-list
						li.nav-header Kyle
						li.active
							a.active(href="#") 
								i.icon-home
								| Profile
								i.icon-chevron-right.pull-right
						li
							a(href="/user/schedules") 
								i.icon-calendar
								| Schedules
								i.icon-chevron-right.pull-right

				.span9
					div.profile(id=userName)
						.row-fluid.profile-header.no-stretch
						